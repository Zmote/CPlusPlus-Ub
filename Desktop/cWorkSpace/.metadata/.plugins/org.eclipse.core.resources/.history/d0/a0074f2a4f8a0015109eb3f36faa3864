#include "wlist.h"
#include "word.h"
#include <vector>
#include <iterator>
#include <algorithm>
#include <iostream>
#include <sstream>

void wlist(std::ostream &out, std::istream &in){
	using namespace std;
	Word myword{};
	in >> myword;
	stringstream ss{};
	ss << myword;
	vector<string> vec{istream_iterator<string>(ss),istream_iterator<string>()};
	for_each(begin(vec),end(vec),[&](auto i){
		out << i;
	});
}
