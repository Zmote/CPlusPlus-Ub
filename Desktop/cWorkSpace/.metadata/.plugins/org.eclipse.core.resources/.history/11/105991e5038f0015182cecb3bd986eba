#include "prime.h"
#include <vector>
#include <algorithm>
#include <functional>
#include <iterator>
#include <iostream>

bool isPrime(const int number){
std::vector<int> range(number);
int n{1};
generate(begin(range),end(range),[&n](){
	return n++;
});
count_if(begin(range),end(range),[&](int i){
	if(number%i == 0){
		divisions++;
	}
});
return divisions >=3? false:true;
}
