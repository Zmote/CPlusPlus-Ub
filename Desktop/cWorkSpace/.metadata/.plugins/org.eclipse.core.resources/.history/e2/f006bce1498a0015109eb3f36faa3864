#include "wlist.h"
#include "word.h"
#include <vector>
#include <iterator>
#include <algorithm>
#include <iostream>
#include <sstream>

void wlist(std::ostream &out, std::istream &in){
	using namespace std;
	Word mywords{};
	in >> mywords;
	ostringstream o{};
	o << mywords;
	istringstream istr{o.str()};
	vector<string> vec{begin(istr),end()};
}
