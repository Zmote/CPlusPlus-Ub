#include "prime.h"
#include <vector>
#include <algorithm>
#include <functional>
#include <iterator>
#include <iostream>

bool isPrime(const int number){
std::vector<int> range(number);
int n{1};
generate(begin(range),end(range),[&n](){
	return n++;
});
int count = count_if(begin(range),end(range),[&](int i){return number%i == 0});
});
return divisions >=3? false:true;
}
