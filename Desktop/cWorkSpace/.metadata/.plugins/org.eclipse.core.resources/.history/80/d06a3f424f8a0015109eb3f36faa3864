#include "wlist.h"
#include "word.h"
#include <vector>
#include <iterator>
#include <algorithm>
#include <iostream>
#include <sstream>

void wlist(std::ostream &out, std::istream &in){
	using namespace std;
	Word myword{};
	in >> myword;
	stringstream ss{};
	ss << myword;
	vector<Word> vec{istream_iterator<Word>(ss),istream_iterator<Word>()};
}
