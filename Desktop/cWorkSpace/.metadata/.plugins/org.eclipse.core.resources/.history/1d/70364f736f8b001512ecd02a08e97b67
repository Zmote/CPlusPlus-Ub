#include "palindrom.h"
#include <iostream>
#include <fstream>

int main(){
std:: string line;
std::fstream myfile ("linuxwords.sql");
 if (myfile.is_open())
 {
   while ( getline (myfile,line) )
   {
	   if(is_palindrom(line)){
	   	std::cout<< line + " is a palindrom";
	   }
	   }
   }
   myfile.close();
 }
}
