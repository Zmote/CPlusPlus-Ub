#include "prime.h"
#include <vector>
#include <algorithm>
#include <functional>

bool isPrime(const int number){
std::vector<int> range{number};
int n{0};
generate(begin(range),end(range),[&n](){
	return n++;
});
int divisions{0};
for_each(begin(range),end(range),[&divisions](int i){
	if(i%j == 0){
				divisions++;
	}
});
}
