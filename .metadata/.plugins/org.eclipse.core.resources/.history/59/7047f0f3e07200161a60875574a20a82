#include <stdio.h>
#include <stdlib.h>

int main(void){
	printf("%s","Before struct output...");
	typedef struct{
		char * name;
		char * surname;
		int age;
		char * description;
	}Person;

	Person zafer;
	zafer.name = "Zafer";
	zafer.surname = "Dogan";
	zafer.age = 27;
	zafer.description = "He is an awesome dude";

	printf("%s %s %i %s", zafer.name, zafer.surname,zafer.age,zafer.description);


	return 0;
}
