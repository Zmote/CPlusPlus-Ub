#include <wx/wx.h>
#include "chat_ui.h"
class my_first_app : public wxApp
{
public :
	bool OnInit()
	{
		globalFrame * mywindow = new globalFrame(NULL,-1,"Zafer's Point-to-Point Chatter");
		mywindow->Show();
		return true;
	}
private:
	wxString session_name{};
	wxString session_host{};
	wxString session_host_port{};
	wxString session_other_host{};
};

IMPLEMENT_APP(my_first_app);
