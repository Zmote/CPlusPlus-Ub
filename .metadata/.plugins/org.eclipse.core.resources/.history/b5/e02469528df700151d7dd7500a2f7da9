#include "BoundedBuffer.h"
#include <iostream>

int main(){
	BoundedBuffer<int> buffer{23};

	buffer.push(1);
	buffer.push(2);

	buffer.push_many(10,23,345,34,34);
	std::cout << "buffer size: " << buffer.size() << "\n";

	for(unsigned int i = 0; i < buffer.size();i++){
		std::cout << buffer.front() << "\n";
		buffer.pop();
	}
}
