#include "calculator.h"
#include <ostream>

int calc(int a,int b,char op){
	switch(op){
		case '+': return (a + b);break;
		case '-': return (a - b);break;
		case '/': return (a/b);break;
		case '*': return (a*b);break;
		default: return 0;
	}
}

void printf(std::ostream& out, int number){
	out << number;
}
